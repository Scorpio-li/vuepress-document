{
    "git.ignoreLimitWarning": true,
    "[markdown]": {
        "editor.formatOnSave": true,
        "editor.renderWhitespace": "all",
        "editor.quickSuggestions": {
            "other": true,
            "comments": true,
            "strings": true
        },
        "editor.acceptSuggestionOnEnter": "on"
    },
    "vscode.tsc.compiler.alertTSConfigChanges": "never"
}